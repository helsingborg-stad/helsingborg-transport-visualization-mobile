{
  "name": "helsingborgapp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start -c",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint",
    "lint:fix": "eslint src/ --fix",
    "lint:style": "stylelint src/",
    "prepare": "husky install",
    "build:development": "eas build --profile development --platform all",
    "build:development:android": "eas build --profile development --platform android",
    "build:development:ios": "eas build --profile development --platform ios",
    "build:staging": "eas build --profile staging --platform all",
    "build:staging:android": "eas build --profile staging --platform android",
    "build:staging:ios": "eas build --profile staging --platform ios",
    "build:production": "eas build --profile production --platform all",
    "build:production:android": "eas build --profile production --platform android",
    "build:production:ios": "eas build --profile production --platform ios",
    "submit": "eas submit --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.2.3",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@turf/turf": "^6.5.0",
    "@types/react": "~18.2.14",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "axios": "^1.4.0",
    "axios-mock-adapter": "^1.21.4",
    "dotenv": "^16.0.3",
    "expo": "^49.0.22",
    "expo-background-fetch": "~11.3.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.13",
    "expo-intent-launcher": "~10.7.0",
    "expo-location": "~16.1.0",
    "expo-notifications": "~0.20.1",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-task-manager": "~11.3.0",
    "expo-updates": "~0.18.19",
    "geojson": "^0.5.0",
    "jwt-decode": "^3.1.2",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-battery-optimization-check": "^1.0.8",
    "react-native-device-info": "^10.12.0",
    "react-native-maps": "1.7.1",
    "react-native-pager-view": "6.2.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-uuid": "^2.0.1",
    "react-native-wheely": "^0.6.0",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.1.3"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src/ --cache --fix",
      "stylelint src/"
    ]
  }
}
